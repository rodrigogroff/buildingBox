@{
	// Build version
	var assembly = System.Reflection.Assembly.GetAssembly(typeof(Application));
	var version = assembly.GetName().Version.ToString(3);
}
<!DOCTYPE html>
<html lang="en" ng-app="app">
<head>
	<base href="/">
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title ng-bind="title">Building Box Office Management</title>
	<link href="https://fonts.googleapis.com/css?family=Open+Sans" rel="stylesheet">
	<meta name="description">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	@Styles.Render("~/bundles/css")
	@Scripts.Render("~/bundles/scripts")
</head>
<body ng-controller="HomeController">	
	<div ng-show="loggedIn == true">
		<div ng-show="loggedInClient == true">
			<table width="100%">
				<tr>
					<td valign="top" width="250px">
						<div>
							<br />
							<br />
							<img style="padding-left:30px;max-height:80px" src='../../images/logoLetras.png' /><br />
							<br />
							<img style="padding-left:20px;max-height:180px"  src='../../images/logoRegister.png' /><br />
						</div>
					</td>
					<td>
						<tabset class="tabs" style="min-height:320px;font-size:13px;width:850px;padding-top:40px">
							<tab heading="Tickets">
								<div style="background:rgba(15, 43, 89, 0.35);min-height:500px;color:black">
									
								</div>
							</tab>
							<tab heading="Contracts">
								<div style="background:rgba(15, 43, 89, 0.35);min-height:500px;color:black">
									x
								</div>
							</tab>
							<tab heading="Customizations">
								<div style="background:rgba(15, 43, 89, 0.35);min-height:500px;color:black">
									x
								</div>
							</tab>
							<tab heading="Setup">
								<div style="background:rgba(15, 43, 89, 0.35);min-height:500px;color:black">
									x
								</div>
							</tab>
						</tabset>
					</td>
				</tr>
			</table>
		</div>
		<div ng-show="loggedInUser == true">

		</div>
	</div>
	<div ng-show="loggedIn == undefined">
		<div align="center">
			<br />
			<a ui-sref="home" class="btn btn-sm {{btnHomeStyle}}" style="color:black">Home</a>&nbsp;&nbsp;&nbsp;
			<a ui-sref="customization" class="btn btn-sm {{btnCustomStyle}}" style="color:black">Customizations</a>&nbsp;&nbsp;&nbsp;
			<a ui-sref="simulation" class="btn btn-sm {{btnSimStyle}}" style="color:black">Infrastructure</a>&nbsp;&nbsp;&nbsp;
			<a ui-sref="features" class="btn btn-sm {{btnFeatureStyle}}" style="color:black">Features</a>&nbsp;&nbsp;&nbsp;
			<a ui-sref="login" class="btn btn-sm btn-primary" style="color:white">Login</a>&nbsp;&nbsp;&nbsp;
			<a ui-sref="register" class="btn btn-sm btn-success" style="color:white">Register</a>&nbsp;&nbsp;&nbsp;
			<br />
			<br />
		</div>
		<div ng-show="showLogo" align="center">
			<div class="animated fadeInLeftBig">
				<img src='../../images/logoBig.png' />
			</div>
		</div>
		<div id="about" class="widget widget-white">
			<div align="center">
				<div id="ui-view" style="width:100%" ui-view></div>
			</div>
		</div>
	</div>
	@Scripts.Render("~/bundles/app")
	<div align="center" ng-show="loggedIn != true">
		<br />
		<h3 style="color:white"> Building Box @@ 2017</h3>
	</div>
</body>
</html>